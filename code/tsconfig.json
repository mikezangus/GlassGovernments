{
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "moduleResolution": "node"
    },
    "include": [
        "api/**/*",
        "pipelines/**/*",
    ]
}
